{
    "name": "@federation-interservices-d-informatique/fiibot-moderation",
    "version": "1.1.2",
    "description": "Common template for FII bots",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "test": "node --experimental-vm-modules node_modules/.bin/jest --passWithNoTests",
        "lint": "eslint . -c .eslintrc.cjs",
        "lint-fix": "eslint . -c .eslintrc.cjs --fix",
        "format": "prettier 'src/**/*.ts' --write",
        "start-build": "tsc && node dist/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/federation-interservices-d-informatique/fiibot-moderation.git"
    },
    "keywords": [
        "fii",
        "discord-bot"
    ],
    "author": "FII Dev team & contributors",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/federation-interservices-d-informatique/fiibot-moderation/issues"
    },
    "homepage": "https://github.com/federation-interservices-d-informatique/fiibot-template#readme",
    "devDependencies": {
        "@types/jest": "^27.0.3",
        "@types/node": "^17.0.5",
        "@typescript-eslint/eslint-plugin": "^5.8.1",
        "@typescript-eslint/parser": "^5.8.1",
        "eslint": "^8.5.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.4.5",
        "prettier": "^2.5.1",
        "ts-jest": "^27.1.2",
        "typescript": "^4.5.4"
    },
    "dependencies": {
        "@federation-interservices-d-informatique/fiibot-common": "^3.0.1",
        "discord.js": "^13.5.0",
        "node-fetch": "^3.1.0",
        "tedis": "^0.1.12"
    }
}
